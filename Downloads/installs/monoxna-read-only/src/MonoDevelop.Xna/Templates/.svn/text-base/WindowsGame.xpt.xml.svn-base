<?xml version="1.0"?>
<Template originator="Lars Magnusson" created="04/04/2009" lastModified="10/07/2009">
	
	<!-- Template Header -->
	<TemplateConfiguration>
		<_Name>Windows Game</_Name>
		<_Category>XNA</_Category>
		<Icon>res:XnaGame_48x48.png</Icon>
		<LanguageName>*</LanguageName>
		<_Description>Creates an empty XNA Game project.</_Description>
	</TemplateConfiguration>
	
	<!-- Actions -->
	<Actions>
	</Actions>
	
	<!-- Template Content -->
	<Combine name="${ProjectName}" directory=".">
		<Options>
			<StartupProject>${ProjectName}</StartupProject>
		</Options>
		
		<Project name="${ProjectName}" directory="." type="XnaProject">
			<!-- Is it possible to set the OutputType without using the Target attribute? -->
			<Options Target="WinExe">
			</Options>
			<References>
				<Reference type="Gac" refto="System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
				<Reference type="Gac" refto="Microsoft.Xna.Framework, Version=0.1.0.33773, Culture=neutral, PublicKeyToken=1c633ed318da3899" />
				<Reference type="Gac" refto="Microsoft.Xna.Framework.Game, Version=0.1.0.17618, Culture=neutral, PublicKeyToken=dc5864553cefd93f" />
			</References>
			<Files>
			</Files>
		</Project>
		
	</Combine>
</Template>